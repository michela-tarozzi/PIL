<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="it.unibo.tw.hibernate.AddizionaleComunale" table="addizionaleComunale">
    <id name="id" column="id">
    </id>
    <property name="codice" column="codice"/>
    <property name="comune" column="comune"/>
    <property name="anno" column="anno"/>
    <property name="sogliaMinima" column="sogliaMinima"/>
    <property name="sogliaMassima" column="sogliaMassima"/>
    <property name="aliquota" column="aliquota"/>
  </class>
</hibernate-mapping>