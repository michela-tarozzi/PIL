<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<?import javafx.scene.control.TextField?>
<TabPane xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.controllerAnagraficaAddizionaleComunale" >
   <tabs>
      <Tab text="Anagrafica degli addizionali comunali" onClosed="#tornaHome">
         <content>
            <AnchorPane >
               <children>
                  <TableView fx:id="tableANAGRAFICA" layoutX="24.0" layoutY="50.0" prefHeight="400">
                    <columns>
                      <TableColumn fx:id="ColonnaCODICE" prefWidth="89.0" text="CODICE" />
                      <TableColumn fx:id="ColonnaCOMUNE" prefWidth="89.0" text="COMUNE" />
                      <TableColumn fx:id="ColonnaANNO" prefWidth="89.0" text="ANNO" />
                      <TableColumn fx:id="colonnaFASCIAZERO" prefWidth="89.0" text="FASCIAZERO" />
                        <TableColumn fx:id="colonnaALIQUOTAZERO" prefWidth="89.0" text="ALIQUOTAZERO" />
                        <TableColumn fx:id="colonnaFASCIAUNO" prefWidth="89.0" text="FASCIAUNO" />
                        <TableColumn fx:id="colonnaALIQUOTAUNO" prefWidth="89.0" text="ALIQUOTAUNO" />
                        <TableColumn fx:id="colonnaFASCIADUE" prefWidth="89.0" text="FASCIADUE" />
                        <TableColumn fx:id="colonnaALIQUOTADUE" prefWidth="89.0" text="ALIQUOTADUE" />
                        <TableColumn fx:id="colonnaFASCIATRE" prefWidth="89.0" text="FASCIATRE" />
                        <TableColumn fx:id="colonnaALIQUOTATRE" prefWidth="89.0" text="ALIQUOTATRE" />
                        <TableColumn fx:id="colonnaFASCIAQUATTRO" prefWidth="89.0" text="FASCIAQUATTRO" />
                        <TableColumn fx:id="colonnaALIQUOTAQUATTRO" prefWidth="89.0" text="ALIQUOTAQUATTRO" />
                        <TableColumn fx:id="colonnaFASCIACINQUE" prefWidth="89.0" text="FASCIACINQUE" />
                        <TableColumn fx:id="colonnaALIQUOTACINQUE" prefWidth="89.0" text="ALIQUOTACINQUE" />
                        <TableColumn fx:id="colonnaSOGLIAESENTE" prefWidth="89.0" text="SOGLIAESENTE" />
                    </columns>
                  </TableView>
                  <Button fx:id="btnNuovoADDIZIONALE" layoutX="24.0" layoutY="14.0" mnemonicParsing="false" onAction="#InserisciNuovoAddizionale" text="Inserisci nuovo addizionale" />
                   <Button fx:id="btnCalcolaAddizionali" layoutX="250.0" layoutY="14.0" mnemonicParsing="false" onAction="#CalcolaAddizionali" text="Calcola Addizionali Comunali" />
                    <TextField fx:id="txtAnno" layoutX="500.0" layoutY="14.0" ></TextField>
               </children>
            </AnchorPane>
         </content>
      </Tab>
   </tabs>
</TabPane>
