<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<TabPane xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.controllerAnagraficaRimborsi">
   <tabs>
      <Tab onClosed="#tornaHome" text="Anagrafica dei soci">
         <content>
            <AnchorPane prefHeight="700.0" prefWidth="1200.0">
               <children>
                  <TableView fx:id="tableANAGRAFICA" layoutX="24.0" layoutY="50.0" prefHeight="625.0" prefWidth="1157.0">
                    <columns>
                        <TableColumn fx:id="ColonnaSPESA" prefWidth="89.0" text="SPESA" />
                        <TableColumn fx:id="ColonnaREGOLA" prefWidth="89.0" text="REGOLA" />
                      <TableColumn fx:id="ColonnaDATA" prefWidth="89.0" text="DATA" />
                      <TableColumn fx:id="ColonnaIMPORTO" prefWidth="89.0" text="IMPORTO" />
                      <TableColumn fx:id="ColonnaIMPORTOSPESA" prefWidth="155.0" text="IMPORTO SPESA" />
                        <TableColumn fx:id="ColonnaPAGAMENTO" prefWidth="89.0" text="PAGAMENTO" />

                    </columns>
                  </TableView>
                  <Button fx:id="btnNuovoSocio" layoutX="24.0" layoutY="14.0" mnemonicParsing="false" onAction="#InserisciNuovoRimborso" text="Inserisci nuovo Rimborso" />
               </children>
            </AnchorPane>
         </content>
      </Tab>
   </tabs>
</TabPane>
